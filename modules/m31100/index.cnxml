        </list>
        <list id="id7506821" list-type="bulleted">
          <item>-7<emphasis effect="italics">a</emphasis> + 3 – (6<emphasis effect="italics">a</emphasis> – 9)</item>
        </list>
        <para id="id12619411">= -7<emphasis effect="italics">a</emphasis> +3 – 6<emphasis effect="italics">a</emphasis> + 9</para>
        <para id="id8475362">= -13<emphasis effect="italics">a</emphasis> + 12</para>
        <list id="id14301412" list-type="bulleted">
          <item>18<emphasis effect="italics">a</emphasis><sup> 2</sup> – 15 – (-7<emphasis effect="italics">a</emphasis><sup> 2</sup> – 5<emphasis effect="italics">a</emphasis> + 8)</item>
        </list>
        <para id="id12365968">= 18<emphasis effect="italics">a</emphasis><sup> 2</sup> – 15 + -7<emphasis effect="italics">a</emphasis><sup> 2</sup> + 5<emphasis effect="italics">a</emphasis> – 8</para>
        <para id="id15889025">= 25<emphasis effect="italics">a</emphasis><sup> 2</sup> + 5<emphasis effect="italics">a</emphasis> – 23</para>
        <list id="id10989120" list-type="bulleted">
          <item>-15<emphasis effect="italics">x</emphasis><sup>2</sup> – 7<emphasis effect="italics">x </emphasis>+ 20 – (-6<emphasis effect="italics">x</emphasis><sup>2</sup> + 76)</item>
        </list>
        <para id="id15154972">= -15<emphasis effect="italics">x</emphasis><sup>2</sup> – 7<emphasis effect="italics">x</emphasis> + 20 + -6<emphasis effect="italics">x</emphasis><sup>2</sup> – 76</para>
        <para id="id13542178">= -9<emphasis effect="italics">x</emphasis><sup>2</sup> – 7<emphasis effect="italics">x</emphasis> – 56</para>
      </section>
      <section id="id15220025">
